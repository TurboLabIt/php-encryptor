{
  "name": "turbolabit/php-encryptor",
  "description": "A single, zero-config service with encrypt() and decrypt() methods to safely share or expose confidential data",
  "type": "symfony-bundle",
  "license": "CC-BY-4.0",
  "authors": [
    {
      "name": "Zane",
      "email": "info@turbolab.it"
    }
  ],
  "require": {
    "php": "^8.2",
    "ext-openssl": "*"
  },
  "autoload": {
    "psr-4": {
      "TurboLabIt\\Encryptor\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "TurboLabIt\\Encryptor\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "symfony/framework-bundle": "@stable",
    "phpunit/phpunit": "@stable"
  }
}
